{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "pretty": true,
        "sourceMap": true,
        "target": "es5",
        "outDir": "dist",
        "lib": [ "es2015" ]
    },
    "include": [
        "src"
    ],
    "exclude": [
        "dist",
        "node_modules"
    ]
}